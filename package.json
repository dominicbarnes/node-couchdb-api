{
  "name": "couchdb-api",
  "version": "2.0.0",
  "description": "CouchDB API client for node applications",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "debug": "^0.8.0",
    "extend": "^1.2.1",
    "extend-object": "^0.1.0",
    "superagent": "~0.18.2"
  },
  "devDependencies": {
    "expect.js": "~0.3.1",
    "istanbul": "~0.3.0",
    "jshint": "^2.5.0",
    "mocha": "~1.18.2",
    "nock": "~0.44.1",
    "nop": "0.0.0",
    "opn": "^0.1.1"
  },
  "scripts": {
    "lint": "jshint .",
    "pretest": "rm -rf coverage",
    "test": "istanbul test _mocha",
    "open-cov": "opn coverage/lcov-report/index.html",
    "prepublish": "npm run lint && npm test --coverage && istanbul check-coverage --statements 100 --functions 100 --branches 100 --lines 100"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dominicbarnes/node-couchdb-api.git"
  },
  "keywords": [
    "couchdb",
    "client",
    "api"
  ],
  "author": "Dominic Barnes <dominic@dbarnes.info>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dominicbarnes/node-couchdb-api/issues"
  }
}
