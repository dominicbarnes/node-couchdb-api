<div class="navbar">
    <div class="navbar-inner">
        <div class="container-fluid">
            <a href="/" class="brand">Dominic Barnes</a>

            <ul class="nav">
                <% libs.forEach(function (api) { %>
                    <%
                    var meta = data[api],
                        members = _.sortBy(meta.methods.concat(meta.properties), function (member) {
                            return member.ctx.name;
                        });
                    %>

                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <%= api %>
                            <b class="caret"></b>
                        </a>

                        <ul class="dropdown-menu">
                            <% members.forEach(function (member) { %>
                                <li>
                                    <a href="#<%= member.ctx.string %>">
                                        <%= member.ctx.string %>
                                    </a>
                                </li>
                            <% }); %>
                        </ul>
                    </li>
                <% }); %>
            </ul>
        </div>
    </div>
</div>
